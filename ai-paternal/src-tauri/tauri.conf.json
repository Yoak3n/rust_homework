{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ai-paternal",
  "version": "0.1.0",
  "identifier": "com.ai-paternal.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:10420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ai-paternal",
        "width": 520,
        "height": 720,
        "resizable": false,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
